{
    "name": "tt-backend",
    "version": "1.0.0",
    "repository": "git@github.com:jpdivinewind/tt-backend.git",
    "author": "jpdivinewind",
    "license": "MIT",
    "scripts": {
        "pretty": "prettier --write src",
        "lint": "eslint --fix src",
        "build": "esbuild src/index.ts --bundle --platform=node --outdir=dist",
        "start": "node dist/index.js",
        "prepare": "husky install"
    },
    "lint-staged": {
      "src/**/*": ["prettier --write", "eslint --fix"]
    },
    "dependencies": {
        "dotenv": "^16.0.3",
        "fastify": "^4.13.0",
        "mongoose": "^6.9.2",
        "mongoose-paginate-v2": "^1.7.1",
        "node-fetch": "^3.3.0",
        "socket.io": "^4.6.0"
    },
    "devDependencies": {
        "@typescript-eslint/eslint-plugin": "^5.52.0",
        "@typescript-eslint/parser": "^5.52.0",
        "esbuild": "^0.17.8",
        "eslint": "^8.34.0",
        "husky": "^8.0.3",
        "lint-staged": "^13.1.2",
        "prettier": "^2.8.4",
        "typescript": "^4.9.5"
    }
}
